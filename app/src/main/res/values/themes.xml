<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MyApplicationTest4" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryVariant">@color/white</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/secodary_dark_theme</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.MyApplicationTest4.MyRipple">
        <item name="colorControlHighlight">@color/white</item>
        <item name="android:background">?selectableItemBackground</item>
    </style>

    <style name="Theme.MyApplicationTest4.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="ActivityTheme">

    </style>

    <style name="Theme.MyApplicationTest4.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.MyApplicationTest4.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="Theme.AccentSecondary"
        parent="ThemeOverlay.AppCompat">
        <item name="colorAccent">@color/white</item>
        <item name="android:colorControlHighlight">@color/white</item>
    </style>
</resources>